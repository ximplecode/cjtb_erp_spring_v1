        <Combo id="cbEPprNm" taborder="8" top="207" height="35" width="233" innerdataset="ds_mk_Paper" datacolumn="PPR_NAME" codecolumn="PPR_CODE" left="edEPprCd:3" font="12px/normal &quot;함초롬바탕 확장&quot;" onitemchanged="cbEPprNm_onitemchanged"/>
