					 "["+this.ed_sgb.value + this.ed_scode.value+"]" +
					 "["+this.ed_egb.value + this.ed_ecode.value+"]" +
					 "["+Sd_sql+"]" +
					 "["+Sd_sql1+"]" + 
					 "["+Sd_sql2+"]" ; //속성코드 자리 
	
	switch(this.cmb_print_select.index)
