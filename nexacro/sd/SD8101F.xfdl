			}
			break;
		case 1:
			if(obj.name == "btn_printview" ) // 미리보기 일때
			{
				var strDs = "ds_1=ds_1";
				strArg += "gubn=0";	
				this.gfn_TransactionP("ds_2","JSP/sd/SD8101F_DS1_SELECT.jsp", strDs, strDs, strArg, "fnDSCallback", true, "P", false, this);
			}
			else if(obj.name == "btn_print")
			{
				rdfile = gv_urlMrd+"/sd/SD81012r.mrd";
				this.lfn_showModal("print", {rdUrl:rdfile, rdParam:rdopt}, "mk::MKRDF.xfdl", "", this, 0, 0, 1400, 760);
			}
			break;
		case 2:
			if(obj.name == "btn_printview" ) // 미리보기 일때
			{
				var strDs = "ds_1=ds_1";
				strArg += "gubn=1";
				this.gfn_TransactionP("ds_3","JSP/sd/SD8101F_DS1_SELECT.jsp", strDs, strDs, strArg, "fnDSCallback", true, "P", false, this);
			}
			else if(obj.name == "btn_print")
			{
				rdfile = gv_urlMrd+"/sd/SD81013r.mrd";
				this.lfn_showModal("print", {rdUrl:rdfile, rdParam:rdopt}, "mk::MKRDF.xfdl", "", this, 0, 0, 1400, 760);
			}
			break;
		case 3: // mrd 존재하지 않음
			/* 
