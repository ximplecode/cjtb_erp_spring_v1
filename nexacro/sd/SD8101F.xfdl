	}
 
 
	// 미리보기 변수
	var strArg = "s_sql="+nexacro.wrapQuote(Sd_sql)+
				 "s_sql1="+nexacro.wrapQuote(Sd_sql1);
	strArg += " syymm="+nexacro.wrapQuote((this.ed_mature_symd.value).substr(0,6));
