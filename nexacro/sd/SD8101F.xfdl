this.fnCallback = function(Trid, ErroCode, ErrorMsg)
{
	if(ErroCode != 0)
		return;
		
	if(Trid == "select")
	{
		var objDate = new nexacro.Date();
		var f_date = Eco.date.getMaskFormatString(objDate, "yyyyMMddhhmmss");
		if(ErroCode != 0)
			trace("[PD160_SearchF.fnCallBack(" + f_date + ")] " + svcid +"::"+ errMsg );
		else
		{
			trace('succ');
		}
	}
}

this.fnDSCallback = function(Trid, ErroCode, ErrorMsg)
{
	if(ErroCode != 0)
		return;

	if(Trid.substr(0,3) == "ds_" && this.ds_1.rowcount <= 0)  // ds_1만 사용
	{
		alert('조회된 데이터가 없습니다.');
		return;
	}
	
	if(Trid == "ds_1")
	{
		this.Div00.set_url();
		this.Div00.set_url("sd::SD8101F_GRID1.xfdl");
	}
	else if(Trid == "ds_2")
	{
		this.Div00.set_url();
		this.Div00.set_url("sd::SD8101F_GRID2.xfdl");
	}
	else if(Trid == "ds_3")
	{
		this.Div00.set_url();
		this.Div00.set_url("sd::SD8101F_GRID3.xfdl");
